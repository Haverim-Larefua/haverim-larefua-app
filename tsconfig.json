{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "jsx": "react-native",
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "sourceMap": true,
    "target": "es2015",
    "lib": ["es7"],
	"skipLibCheck": true,
	"noEmit": true
  },
  "exclude": ["node_modules", "android", "ios"],
  "include": ["index.js", "app", "test", "storybook"]
}
